{
  "expo": {
    "name": "bots",
    "slug": "bots-for-kids",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/syft.png",
    "scheme": "botsforkids",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "sdkVersion": "52.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tpaulshippy.botsforkids",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/syft.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#012b64"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "bots-app",
          "organization": "tpaulshippycom"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c5f43f2f-c295-4b8e-b4ee-fb40063ee003"
      }
    },
    "owner": "tpaulshippy",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c5f43f2f-c295-4b8e-b4ee-fb40063ee003"
    }
  }
}
